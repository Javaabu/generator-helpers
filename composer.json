{
    "name": "javaabu/generator-helpers",
    "description": "Helper methods for Laravel generators",
    "homepage": "https://github.com/Javaabu/generator-helpers",
    "license": "MIT",
    "keywords": [
        "javaabu",
        "laravel",
        "generator_helpers"
    ],
    "authors": [
        {
            "name": "Arushad Ahmed",
            "email": "arushad@javaabu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0 || ^10.0",
        "phpunit/phpunit": "^9.5 || ^10.5 || ^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\GeneratorHelpers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\GeneratorHelpers\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\GeneratorHelpers\\GeneratorHelpersServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
